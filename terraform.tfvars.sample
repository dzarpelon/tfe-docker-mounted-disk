# AWS variables setup
aws_region              = "eu-central-1"
aws_credentials_profile = "default"

# EC2 instance variables
aws_ami           = "ami-07eef52105e8a2059"
aws_instance_type = "t3.medium"

# Tags
aws_owner         = "john.doe" # Example: john.doe
aws_instance_name = "my-tfe-instance"
certbot_email     = "john.doe@example.com"

tfe_version       = "v202503-1"
disk_size         = 80 #this is in GB

route53_zone_id   = "Z1234567890ABCDEF" 
route53_zone_name = "example.com"

encryption_password = "mySecureEncryptionPassword" # this is the encryption password used by TFE, make sure it's a secure one and do not share this publicly.
initial_user_password = "secureUserPassword>" # the initial admin user password must be at least 10 characters long. As any credential, do not share this and make sure it's a safe one.
tfe_license = "" #this an example value and will NOT work as is. Make sure you add a valid license.

